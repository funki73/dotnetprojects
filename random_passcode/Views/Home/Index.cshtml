@{
    ViewData["Title"] = "Passcode";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="~/css/style.css"/>
    <title>Random Generator</title>
</head>
<body>
    <div class="container">
        <div class="title">
            <h1>Random passcode Passcode# @ViewBag.Count</h1>
        </div>
        <div class="main">
            <div>
                <p>@ViewBag.Passwrd</p>
            </div>
            <br>
            <button><a type="submit" href="increment" class="btn btn-dark">Generate</a></button>
        </div>
    </div>
</body>
</html>